    <title>集合论</title>
    <link href="http://example.com/2022/10/01/%E9%9B%86%E5%90%88%E8%AE%BA/"/>
    <id>http://example.com/2022/10/01/%E9%9B%86%E5%90%88%E8%AE%BA/</id>
